#include <stdio.h>
void my_putchar(char c) { write(1, &c, 1); }
void my_print_alphabet() {
    for (int c = 97; c <= 122; ++c) {
        my_putchar((char)c);
    }
    my_putchar('\n');
}

putchar in str:
#include <stdio.h>

void my_putstr(char* a){
        for (int i=0; *(a+i) != '\0'; i++){
            putchar(a[i]);
        }
     //putchar('\n');

}
