nt main(int argc, char *argv[]) {
  FILE *file;
  int j;
 
  for(int i = 1; i < argc; i++) {
    if((file = fopen(argv[i], "r")) == 0) {
      fprintf(stderr, "%s: %s : %s\n", argv[0], argv[i], strerror(errno));
    continue;
    }
    while((j = getc(file)) != EOF)
      fprintf(stdout, "%c", j);
    fclose(file);
  }
  exit(0);
}
