#include <errno.h>
#include <stdio.h>
#include <unistd.h>
#include <stdlib.h>
#include <string.h>
prava:
int main(int argc, char *argv[]) {
  FILE *catfile;
  int j;
  for(int i = 1; i < argc; i++) {
    catfile = fopen(argv[i], "r");
    while((j = getc(catfile)) != EOF)
      fprintf(stdout, "%c", j);
    fclose(catfile);
  }
}
