def my_square(n, m):
  s=''
  s += '0'
  for i in range(m-2):
    s += '-'
  s += '0'
  print(s)
  for i in range(n-2):
    s = ''
    s += '|'
    for j in range(m-2):
      s += ' '
    s += '|'
    print(s)
  s=''
  s += '0'
  for i in range(m-2):
    s += '-'
  s += '0'
  print(s)
  




my_square(4, 5)
