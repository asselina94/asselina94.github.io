#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int my_iterative_pow(int a, int b) {
    if(b==0){
        return 0;
    }
    else{
        int c = 1;
        for(int i=1; i<=b; i++)
    {   
        c = c * a;  
    }
        //printf("%d",c); 
        return c;  
    }
    //printf("%d",c);
    //return c;
}
